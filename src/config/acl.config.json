[
  {
    "group" : "customer",
    "permissions" : [
      {
        "resource" : "*",
        "methods" : "*",
        "action" : "allow"
      }
    ]
  },
  {
    "group": "admin",
    "permissions": [
      {
        "resource": "*",
        "methods": "*",
        "action": "deny"
      }
    ]
  },
  {
    "group": "user",
    "permissions": [
      {
        "resource": "users",
        "methods": ["POST", "GET", "PUT"],
        "action": "deny"
      }
    ]
  }
]